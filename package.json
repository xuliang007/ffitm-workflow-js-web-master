{
  "name": "ffitm-workflow-web",
  "version": "1.0.0",
  "description": "A Vue.js project",
  "author": "ffitm",
  "private": true,
  "scripts": {
    "fix": "eslint --fix src/**/*.{js,vue}",
    "fix-prod": "cross-env NODE_ENV=production eslint --fix src/**/*.{js,vue}",
    "mock-dev": "cross-env USE_MOCK=true webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "start": "npm run dev",
    "lint": "eslint --ext .js,.vue src",
    "build": "node --max-old-space-size=15000 build/build.js",
    "dll:vendor": "node build/build-dll.js vendor",
    "dll:fi": "node build/build-dll.js fi",
    "dll:workflow": "node build/build-dll.js workflow",
    "dll": "npm run dll:vendor && npm run dll:fi && npm run dll:workflow",
    "new": "node build/bin/new.js",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "ffitm-component": "^1.0.0",
    "ffitm-fi-font": "^1.1.4",
    "ffitm-fpr-loader": "^1.0.0",
    "ffitm-fui": "^1.0.43",
    "ffitm-fui-comp-extends": "^1.0.14",
    "ffitm-sso-util": "^1.0.0",
    "ffitm-vue-ui": "^0.27.1-alpha.5",
    "ffitm-workflow-template-ui": "^0.1.3",
    "ffitm-workflow-ui": "^0.1.120",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "moment": "^2.22.2",
    "node-sass": "^4.9.3",
    "throttle-debounce": "^1.0.0",
    "vue": "^2.5.2"
  },
  "devDependencies": {
    "add-asset-html-webpack-plugin": "^2.0.0",
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^8.2.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-2": "^6.22.0",
    "chalk": "^2.0.1",
    "compression-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.0",
    "eslint": "^4.15.0",
    "eslint-config-airbnb-base": "^11.3.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-vue": "^4.0.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "ffitm-ftl-loader": "^1.0.0",
    "file-loader": "^1.1.4",
    "file-save": "^0.2.0",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "glob": "^7.1.3",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "mockjs": "^1.0.1-beta3",
    "node-notifier": "^5.1.2",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "ora": "^1.2.0",
    "portfinder": "^1.0.13",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.8",
    "postcss-url": "^7.2.1",
    "rimraf": "^2.6.0",
    "sass-loader": "^7.1.0",
    "semver": "^5.7.0",
    "shelljs": "^0.7.6",
    "uglifyjs-webpack-plugin": "1.2.7",
    "url-loader": "^0.5.8",
    "vue-loader": "^13.3.0",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.5.2",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.11.5",
    "webpack-merge": "^4.1.0"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "lint-staged": {
    "src/**/*.js": [
      "npm run fix-prod",
      "npm run lint",
      "git add"
    ],
    "src/**/*.vue": [
      "npm run fix-prod",
      "npm run lint",
      "git add"
    ]
  }
}
